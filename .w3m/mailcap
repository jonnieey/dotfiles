application/postscript;	zathura %s; test=test -n "$DISPLAY"
application/eps;	zathura %s; test=test -n "$DISPLAY"
application/x-eps;	zathura %s; test=test -n "$DISPLAY"
image/eps;	zathura %s; test=test -n "$DISPLAY"
image/x-eps;	zathura %s; test=test -n "$DISPLAY"
inode/directory;	ranger %s; needsterminal
x-scheme-handler/spice;	remote-viewer %s; test=test -n "$DISPLAY"
application/x-virt-viewer;	remote-viewer %s; test=test -n "$DISPLAY"
application/x-tint2-theme;	tint2conf %s; test=test -n "$DISPLAY"
text/html;	/usr/bin/w3m -T text/html %s; needsterminal; description=HTML Text; nametemplate=%s.html
text/html;	/usr/bin/lynx -force_html %s; needsterminal; description=HTML Text; nametemplate=%s.html
application/zip;	unzip -l %s; nametemplate=%s.zip; copiousoutput
text/html;	/usr/bin/w3m -I %{charset} -dump -T text/html %s; copiousoutput; description=HTML Text; nametemplate=%s.html
